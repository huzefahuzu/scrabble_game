<section class="mb-5">
  <div class="container">
    <div class="row">
      <div class="col-lg-6 col-xs-12 col-md-6 col-sm-12 p-0">
        <h2 class="pt-1">
          Leader Board
        </h2>
      </div>
    </div>

    <div class="row">
      <div class="col-lg-12 col-xs-12 col-md-12 col-sm-12 p-0">
        <div class="box-card relative">
          <%= render "top_players", players: @players %>

          <div class="digg_pagination">
            <%= will_paginate @players %>
          </div>
        </div>
      </div>
    </div>

  </div>
</section>